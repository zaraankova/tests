{
  "name": "ci-cd",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve  --proxy-config ./proxy.json",
    "server": "ts-node -P ./server/server.tsconfig.json ./server/server.ts",
    "build": "ng build",
    "lint": "ng lint",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "build:prod": "ng build --prod",
    "start:prod": "http-server ./dist -a localhost -p 4200",
    "build-and-start:prod": "run-s build:prod start:prod",
    "e2e": "start-server-and-test build-and-start:prod http://localhost:4200 cypress:run",
    "test": "ng test",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadless"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.2.13",
    "@angular/common": "^20.3.11",
    "@angular/compiler": "^20.3.11",
    "@angular/core": "^20.3.11",
    "@angular/forms": "^20.3.11",
    "@angular/material": "^20.2.13",
    "@angular/platform-browser": "^20.3.11",
    "@angular/platform-browser-dynamic": "^20.3.11",
    "@angular/router": "^20.3.11",
    "body-parser": "^2.2.0",
    "core-js": "^3.46.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "http-server": "^14.1.1",
    "moment": "^2.30.1",
    "rxjs": "~7.8.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.10",
    "@angular/cli": "^20.3.10",
    "@angular/compiler-cli": "^20.3.11",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.3"
  }
}
